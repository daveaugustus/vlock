<?xml version="1.0" encoding="UTF-8"?>
<!-- Fiserv Voltage Configuration - QA Environment -->

<VoltageSecurityConfiguration>
    
    <cryptId name="SSN_Internal" algorithm="FPE" key="YOUR_QA_SSN_KEY_HERE" format="NUMERIC">
        <description>QA SSN encryption</description>
    </cryptId>
    
    <cryptId name="CCN_Internal" algorithm="FPE" key="YOUR_QA_CCN_KEY_HERE" format="NUMERIC">
        <description>QA credit card encryption</description>
    </cryptId>
    
    <cryptId name="EMAIL_Internal" algorithm="FPE" key="YOUR_QA_EMAIL_KEY_HERE" format="ALPHANUMERIC">
        <description>QA email encryption</description>
    </cryptId>
    
    <cryptId name="TEXT_Internal" algorithm="AES256" key="YOUR_QA_TEXT_KEY_HERE" format="BASE64">
        <description>QA general text encryption</description>
    </cryptId>
    
    <mask pattern="XXX-XX-####" cryptId="SSN_Internal"/>
    <mask pattern="XXXX-XXXX-XXXX-####" cryptId="CCN_Internal"/>
    <mask pattern="XXX@XXXXX.XXX" cryptId="EMAIL_Internal"/>
    
    <security>
        <keyRotationEnabled>true</keyRotationEnabled>
        <auditLogging>true</auditLogging>
    </security>
    
</VoltageSecurityConfiguration>
