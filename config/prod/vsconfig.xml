<?xml version="1.0" encoding="UTF-8"?>
<!-- Fiserv Voltage Configuration - Production Environment -->
<!-- CRITICAL: Production keys and configuration -->
<!-- SANITIZED EXAMPLE - Replace with actual production configuration -->

<VoltageSecurityConfiguration>
    
    <!-- PRODUCTION CryptID Definitions -->
    <!-- Use production-specific keys provided by Voltage team -->
    
    <cryptId name="SSN_Internal" algorithm="FPE" key="YOUR_PROD_SSN_KEY_HERE" format="NUMERIC">
        <description>Production SSN encryption</description>
        <keyVersion>v2.1</keyVersion>
        <rotationDate>2025-12-01</rotationDate>
    </cryptId>
    
    <cryptId name="CCN_Internal" algorithm="FPE" key="YOUR_PROD_CCN_KEY_HERE" format="NUMERIC">
        <description>Production credit card encryption (PCI-DSS compliant)</description>
        <keyVersion>v2.1</keyVersion>
        <rotationDate>2025-12-01</rotationDate>
    </cryptId>
    
    <cryptId name="EMAIL_Internal" algorithm="FPE" key="YOUR_PROD_EMAIL_KEY_HERE" format="ALPHANUMERIC">
        <description>Production email encryption</description>
        <keyVersion>v2.1</keyVersion>
        <rotationDate>2025-12-01</rotationDate>
    </cryptId>
    
    <cryptId name="TEXT_Internal" algorithm="AES256" key="YOUR_PROD_TEXT_KEY_HERE" format="BASE64">
        <description>Production general text encryption</description>
        <keyVersion>v2.1</keyVersion>
        <rotationDate>2025-12-01</rotationDate>
    </cryptId>
    
    <cryptId name="BINARY_Internal" algorithm="AES256" key="YOUR_PROD_BINARY_KEY_HERE" format="BINARY">
        <description>Production binary encryption</description>
        <keyVersion>v2.1</keyVersion>
        <rotationDate>2025-12-01</rotationDate>
    </cryptId>
    
    <!-- Masking Patterns -->
    <mask pattern="XXX-XX-####" cryptId="SSN_Internal">
        <description>Mask all but last 4 digits of SSN</description>
    </mask>
    
    <mask pattern="XXXX-XXXX-XXXX-####" cryptId="CCN_Internal">
        <description>Mask all but last 4 digits of credit card</description>
    </mask>
    
    <mask pattern="XXX@XXXXX.XXX" cryptId="EMAIL_Internal">
        <description>Mask email but preserve structure</description>
    </mask>
    
    <!-- Production Security Settings -->
    <security>
        <keyRotationEnabled>true</keyRotationEnabled>
        <keyRotationIntervalDays>60</keyRotationIntervalDays>
        <auditLogging>true</auditLogging>
        <complianceMode>PCI-DSS</complianceMode>
        <failOnInvalidKey>true</failOnInvalidKey>
    </security>
    
    <!-- Audit Configuration -->
    <audit>
        <logAllOperations>true</logAllOperations>
        <logEncryption>true</logEncryption>
        <logDecryption>true</logDecryption>
        <logKeyAccess>true</logKeyAccess>
    </audit>
    
</VoltageSecurityConfiguration>
